        xmlns:local="clr-namespace:TgimbaWpfClient.Views"
             >
    <Grid>
        <TextBox x:Name="tbSearchTerm" Height="23" Margin="37,13,34,169" TextWrapping="Wrap" Text="SearchTerm"  Width="120"/>
